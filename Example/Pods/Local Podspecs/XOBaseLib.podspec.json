  "homepage": "http://192.168.1.119/xxoogo_livechat/xobaselib",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "kenter": "Hjt_830@163.com"
  },
  "source": {
    "git": "http://192.168.1.119/xxoogo_livechat/xobaselib.git",
    "tag": "0.0.1"
  },
  "social_media_url": "https://www.jianshu.com/u/7e5e59276b03",
  "platforms": {
    "ios": "8.0"
